#: la_petite_portugaise/templates/about-us.html:36
msgid "Books in Portuguese"
msgstr "Des livres en portugais"

#: la_petite_portugaise/templates/about-us.html:37
msgid ""
"Books about Portugal and about Portuguese-speaking countries and cultures"
msgstr "Des livres sur le Portugal et sur les pays et cultures Lusophones"

#: la_petite_portugaise/templates/about-us.html:38
msgid "Portuguese products"
msgstr "Des produits portugais"

#: la_petite_portugaise/templates/about-us.html:39
msgid "Various cultural events."
msgstr "Des événements culturels variés"

#: la_petite_portugaise/templates/about-us.html:41
msgid ""
